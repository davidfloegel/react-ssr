{
  "name": "minimal-react-webpack-babel-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon", 
    "start": "NODE_ENV=production node build/lib/server/index.js",
    "build": "NODE_ENV=production yarn build:webpack && yarn build:lib",
    "build:webpack": "NODE_ENV=production webpack",
    "build:lib": "babel -d build/lib src --extensions '.js,.jsx,.ts,.tsx'",
    "prebuild": "yarn type-check && yarn lint",
    "type-check": "tsc --noEmit",
    "lint": "tslint -c tslint.json -p tsconfig.json --format stylish",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "wdio": "wdio wdio.conf.js",
    "wdio2": "wdio wdio.conf.js; wdio_ret=$?; forever stopall; exit $wdio_ret",
    "bdd": "./node_modules/.bin/cucumber-js bdd/features -f node_modules/cucumber-pretty --require-module @babel/register --require 'bdd/step_definitions/**/*.js'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
